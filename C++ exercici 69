#include <iostream>                                                          
using namespace std;
                                         
int main(){                                                            
    int *punt;                                                 
    int x=7;                                                  
    int y=5;                                            
    punt=&x;                                                
    *punt=3;                                                
    punt=&y;                                                
    *punt=x;                                                
    x=9;                                                
    cout << *punt << " " << x << " " << y << endl;                                                    
}

R: 393
