#include <iostream>
#include <string>

using namespace std;

int sum (int* a, int n);
int main(){
    int* a;
    int n;
    cin >> n;
    a = new int[n]; 
    for (int i = 0; i < n; i++){
        cin >> a[i];   
    }
    if (n == 0){
        cout << 0;
    }else{
        cout << sum(a, n-1);
    }    
    return 0;
}

int sum(int* a, int n){
    if(n == 0){
        return a[0];
    }else{
        return a[n] + sum(a, n-1);
    }
}
