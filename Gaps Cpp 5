#include <iostream>
#include <string>
using namespace std;

unsigned int multiply(unsigned int a, unsigned int b);

int main(){
    int a, b;
    cin >> a >> b;
    cout << multiply(a, b);
    return 0;
}

unsigned int multiply(unsigned int a, unsigned int b){
    if(b == 0){
        return 0;
    }else{
        return a + multiply(a, b - 1);
    }
}
