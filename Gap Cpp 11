#include <iostream>
using namespace std;

int sumNumbers(string s);
int main(){
    string text;
    int total;
    cin >> text;
    total = sumNumbers(text);
    cout << "The sum of the digits in the text " << text << " is " << total;
    return 0;
}

int sumNumbers(string s){
    int suma = 0;
    for(int i = 0; i < s.length(); i=i+1){
        if ('0' <= s[i] && s[i] <= '9'){
          suma = suma + (s[i] - '0');
        }
    }
    return suma;
}
