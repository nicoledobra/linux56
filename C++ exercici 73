#include <iostream>
#include <string>
                                         
using namespace std;
int main(){
    int *punt,i;
    int x[5]={1,2,3,4,5};
    punt=x+4;
    *(punt-2)=9;    
    punt--;
    *(punt)=7 ;
    punt[1]=11 ;
    for(i=0;i<5;i++){
       cout << *(x+i); 
    }
    return 0;                                            
}

